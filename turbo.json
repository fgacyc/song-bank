{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": [".env"],
  "globalDotEnv": [".env"],
  "globalEnv": [
    "NODE_ENV",
    "SKIP_ENV_VALIDATION",
    "DATABASE_URL",
    "NEXT_PUBLIC_AWS_S3_BUCKET",
    "NEXT_PUBLIC_AWS_SECRET_ACCESS_KEY",
    "NEXT_PUBLIC_AWS_REGION",
    "NEXT_PUBLIC_AWS_ACCESS_KEY"
  ]
}
